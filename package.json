{
  "name": "gamehq",
  "version": "0.0.1",
  "description": "A game data aggregator",
  "author": "Bill, John, and Steven",
  "license": "ISC",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build-dev": "cross-env NODE_ENV=development && webpack --mode development",
    "build-prod": "cross-env NODE_ENV=production && webpack --mode production",
    "client": "cross-env NODE_ENV=development && webpack serve --hot --mode development",
    "server": "nodemon server/server.js",
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\" --hot"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.5",
